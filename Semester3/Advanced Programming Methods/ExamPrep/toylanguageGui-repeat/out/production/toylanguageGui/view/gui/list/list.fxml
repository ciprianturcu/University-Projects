<StackPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.gui.list.ListController">
        <VBox alignment="CENTER">
            <children>
                <ListView fx:id="programListView" prefHeight="356.0" prefWidth="525.0" style="-fx-background-color: #CCFBFE; -fx-background-radius: 5px;" />
                <Button fx:id="displayButton" alignment="BASELINE_CENTER" maxHeight="-Infinity" minHeight="-Infinity" mnemonicParsing="false" onAction="#displayProgram" prefHeight="24.0" style="-fx-background-color: #CD8987; -fx-border-color: #6B2737; -fx-border-radius: 15px; -fx-background-radius: 15px;" text="Display program" textAlignment="CENTER" textFill="#6b2737">
                    <font>
                        <Font name="Berlin Sans FB" size="12.0" />
                    </font>
                </Button>
            </children>
        </VBox>
</StackPane>
